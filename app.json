{
  "expo": {
    "name": "GamePlay",
    "slug": "GamePlay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "backgroundColor": "#070b24",
    "scheme": "game-play",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0E1647"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#0E1647"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.eliasgcf.gameplay",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A1033"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
